<h2>Nonprofits</h2>

<ul>
  <% @nonprofits.each do |nonprofit| %>
  <li> <%= link_to nonprofit.name, nonprofit_path(nonprofit) %> </li>
  <% end %>
</ul>



<%= form_for @nonprofit, url: {action: "create"} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.submit "Create" %>
<% end %>
